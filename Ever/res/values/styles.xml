	<!-- PopDownMenu -->
	<style
		name="Animations.PopDownMenu" />

	<style
		name="Animations.PopDownMenu.Center">
		<item
			name="@android:windowEnterAnimation">@anim/grow_from_top</item>
		<item
			name="@android:windowExitAnimation">@anim/shrink_from_bottom</item>
	</style>
	<style
		name="Animations.PopDownMenu.Left">
		<item
			name="@android:windowEnterAnimation">@anim/grow_from_topleft_to_bottomright</item>
		<item
			name="@android:windowExitAnimation">@anim/shrink_from_bottomright_to_topleft</item>
	</style>
	
	
		<style
		name="Animations.PopDownMenu.Right">
		<item
			name="@android:windowEnterAnimation">@anim/grow_from_topright_to_bottomleft</item>
		<item
			name="@android:windowExitAnimation">@anim/shrink_from_bottomleft_to_topright</item>
	</style>

	<style
		name="Animations.PopDownMenu.Reflect">
		<item
			name="@android:windowEnterAnimation">@anim/pump_top</item>
		<item
			name="@android:windowExitAnimation">@anim/disappear</item>
	</style>

